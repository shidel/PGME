.autodepend

# Makefile

ARCH = arch\msdos

BINDIR = ..\BIN

TPC = TPC -M -L -E$(BINDIR) -I..\QUICKCRT -U$(BINDIR);..\QUICKCRT

efntdsgn: efntdsgn.exe	

efnttools: qfntload.exe qfnt2raw.exe raw2qfnt.exe qfnt2tsr.exe

all: efntdsgn efnttools qscroll.exe qcnftool.exe qdetune.exe qkeys.exe qnothing.exe

.pas.tpu: 
    @if not exist $(BINDIR) mkdir $(BINDIR)
	$(TPC) $*.pas

.pas.exe: 
    @if not exist $(BINDIR) mkdir $(BINDIR)
	$(TPC) $*.pas
